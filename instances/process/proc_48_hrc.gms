parameter count;
parameter imax(k) "Actual maximum number of terms per disjunction"
set Eq(k,i,pr);
loop(k,
	count = 0;
	loop(pr,
		if(equip(k,pr),count = count + 1;
		loop(i,
			if(ord(i) eq count, Eq(k,i,pr) = yes; );
		);
		);
	);
	imax(k) = count + 1;
);


loop(k,
     ki(k,i)$((ord(i)<=imax(k)))=yes;
);

